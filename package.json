{
  "name": "rrishmusic",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    
    "test:unit": "vitest run --config ./configs/vitest.config.unit.ts",
    "test:unit:watch": "vitest --config ./configs/vitest.config.unit.ts",
    "test:unit:coverage": "vitest run --config ./configs/vitest.config.unit.ts --coverage",
    "test:unit:ui": "vitest --config ./configs/vitest.config.unit.ts --ui",
    "test:integration": "vitest run --config ./configs/vitest.config.integration.ts",
    "test:integration:watch": "vitest --config ./configs/vitest.config.integration.ts",
    "test:integration:coverage": "vitest run --config ./configs/vitest.config.integration.ts --coverage",
    "test:integration:ui": "vitest --config ./configs/vitest.config.integration.ts --ui",
    "test:e2e": "playwright test --config ./configs/playwright.config.ts",
    "test:e2e:ui": "playwright test --config ./configs/playwright.config.ts --ui",
    "test:e2e:debug": "playwright test --config ./configs/playwright.config.ts --debug",
    "test:e2e:headed": "playwright test --config ./configs/playwright.config.ts --headed",
    "test:e2e:report": "playwright show-report ./test-results/e2e-report",
    "test:e2e:install": "playwright install",
    "test:e2e:chromium": "playwright test --config ./configs/playwright.config.ts --project=chromium",
    "test:e2e:firefox": "playwright test --config ./configs/playwright.config.ts --project=firefox",
    "test:e2e:webkit": "playwright test --config ./configs/playwright.config.ts --project=webkit",
    "test:e2e:mobile": "playwright test --config ./configs/playwright.config.ts --project='Mobile Chrome'",
    "test:performance": "vitest run --config ./configs/vitest.config.performance.ts --reporter=verbose",
    "test:performance:watch": "vitest --config ./configs/vitest.config.performance.ts",
    "test:performance:hooks": "vitest run --config ./configs/vitest.config.performance.ts src/content/__tests__/performance.test.ts",
    "test:performance:components": "vitest run --config ./configs/vitest.config.performance.ts tests/performance/component-rendering.perf.test.ts",
    "test:performance:bundle": "vitest run --config ./configs/vitest.config.performance.ts tests/performance/bundle-analysis.test.ts",
    "test:performance:memory": "vitest run --config ./configs/vitest.config.performance.ts tests/performance/memory-usage.test.ts",
    "test:performance:vitals": "vitest run --config ./configs/vitest.config.performance.ts tests/performance/web-vitals.test.ts",
    "test:performance:lighthouse": "vitest run --config ./configs/vitest.config.performance.ts tests/performance/lighthouse.test.ts",
    "test:a11y": "vitest run --config ./configs/vitest.config.accessibility.ts",
    "test:a11y:watch": "vitest --config ./configs/vitest.config.accessibility.ts",
    "test:a11y:audit": "vitest run --config ./configs/vitest.config.accessibility.ts --reporter=verbose",
    "test:a11y:coverage": "vitest run --config ./configs/vitest.config.accessibility.ts --coverage",
    "test:visual": "playwright test --config ./configs/playwright.config.visual.ts",
    "test:visual:update": "playwright test --config ./configs/playwright.config.visual.ts --update-snapshots",
    "test:visual:ui": "playwright test --config ./configs/playwright.config.visual.ts --ui",
    "test:visual:chromium": "playwright test --config ./configs/playwright.config.visual.ts --project=chromium",
    
    "test": "npm run test:unit",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:performance && npm run test:a11y",
    "test:parallel": "concurrently \"npm run test:unit\" \"npm run test:integration\" \"npm run test:performance\"",
    "test:ui": "npm run test:unit:ui",
    "test:coverage": "npm run test:unit:coverage",
    
    "test:fast": "npm run test:unit && npm run test:quick && npm run type-check && npm run lint && npm run build",
    "test:ci": "timeout 300 npm run test:fast",
    "test:quick": "playwright test --config ./configs/playwright.config.ts tests/e2e/smoke.spec.ts --project=chromium",
    
    "test:slow": "npm run test:e2e && npm run test:performance && npm run test:a11y:audit && npm run test:visual",
    "test:full": "npm run test:fast && npm run test:slow",
    "test:manual": "npm run test:slow",
    
    "test:e2e:smoke": "playwright test --config ./configs/playwright.config.ts tests/e2e/smoke.spec.ts --project=chromium --timeout=30000",
    "test:e2e:critical": "playwright test --config ./configs/playwright.config.ts tests/e2e/critical-path.spec.ts --project=chromium --timeout=60000",
    "test:performance:quick": "vitest run --config ./configs/vitest.config.performance.ts tests/performance/bundle-analysis.test.ts --timeout=120000",
    
    "benchmark": "node scripts/performance-benchmark.js",
    "benchmark:ci": "node scripts/performance-benchmark.js --ci",
    "benchmark:quick": "node scripts/performance-benchmark.js --no-lighthouse",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=test-results/reports/lighthouse-report.html",
    "lighthouse:ci": "lighthouse http://localhost:3000 --output=json --output-path=test-results/reports/lighthouse-ci.json",
    "bundle:analyze": "npx vite-bundle-analyzer dist/",
    "bundle:size": "du -sh dist/ && find dist -name '*.js' -o -name '*.css' | xargs ls -lh",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write src/",
    "test-runner": "node scripts/test-runner.js"
  },
  "dependencies": {
    "framer-motion": "^12.23.12",
    "npm": "^11.5.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.2",
    "concurrently": "^9.1.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.12.0",
    "jsdom": "^26.0.1",
    "lighthouse": "^12.3.0",
    "lighthouse-ci": "^0.13.0",
    "picocolors": "^1.1.2",
    "playwright": "^1.55.0",
    "postcss": "^8.6.0",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.5.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.40.0",
    "vite": "^5.4.14",
    "vite-bundle-analyzer": "^0.18.0",
    "vitest": "^3.2.4"
  }
}